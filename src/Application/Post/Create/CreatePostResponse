<?php

namespace App\Application\Post\Create;

use App\Domain\Entity\Post;

class CreatePostResponse
{
    private Post $post;

    /**
     * @param Post $post
     */
    public function __construct(Post $post)
    {
        $this->post = $post;
    }

    /**
     * @return Post
     */
    public function getPost(): Post
    {
        return $this->post;
    }
}
